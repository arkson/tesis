<script>
$(function() {
	$( "#dialog:ui-dialog" ).dialog( "destroy" );		
	
	$( "#dialog-form" ).dialog({
		   autoOpen: false,
		   modal: true,
		   title: "CONFIRMACION DE ALQUILER",
		   width: 550,
		   minWidth: 400,
		   maxWidth: 650,
		   closeText: "cerrar",
		   show: "fade"
	});


	$( "#confirmar" )
		.button()
		.click(function() {
			$( "#dialog-form" ).dialog( "open" );
		});


});
</script>



<div class="demo">

<div id="dialog-form" class="login">
	<p>
		<b>Datos del Usuario</b>
	</p>

  	
</div>

</div>
