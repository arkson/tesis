

<div class ="titulo"><h1>Alquileres realizados</h1></div>

<table class="tabla_scaffold" width="75%">
	<% @alquileres.each do |alquiler| %>
	<tr class="<%= cycle('list_line_odd', 'list_line_even') %>">

		<td class="list_description">
			<dl>
				<dd>Fecha de inicio: <%= Date.strptime("31/01/2010", "%d/%m/%Y") %></dd>
				<dd>Fecha de culminaci√≥n: <%= alquiler.fecha_fin %></dd>
				<dd>Estatus: <%= alquiler.estatus %></dd>
			</dl>
		</td>
		<td class="ver_datos" width="10%">
			<%= link_to 'Ver Datos', ppal_estudiante_ver_datos_path(:id => alquiler.id),:id => "links" %>
		</td>
	</tr>
	<% end %>
</table>


