
  

<%= will_paginate @historias , :prev_label => "Anterior", :next_label => "Siguiente" %>

 <table>


<% @historias.each do |historia| %>
  <tr>
    <td><%= sanitize(historia.titulo) %></td></br>
    <td> <%= image_tag(historia.imagen.url(:thumb),:class => 'list_image') %></td>
    <td><%= sanitize(historia.contenido) %></td>
     </tr>
<% end %>
</table>

