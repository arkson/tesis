<% if notice %>
<p class="notice"><%= notice %></p>
<% end %>
<div class = "mostrar">
<p>
  <b>Nombre:</b>
  <%= @usuario.nombre %>
</p>

<p>
  <b>Cedula:</b>
  <%= @usuario.cedula %>
</p>

<p>
  <b>Correo:</b>
  <%= @usuario.correo %>
</p>

<p>
  <b>Unid creditos:</b>
  <%= @usuario.unid_creditos %>
</p>

<p>
  <b>Dependencia:</b>
  <%= link_to @usuario.dependencia.nombre, @usuario.dependencia %>
</p>
</div>
<br /> 
<%= link_to 'Editar', edit_usuario_path(@usuario), :id => "links" %> &nbsp
<%= link_to 'Regresar', :back , :id => "links" %>

