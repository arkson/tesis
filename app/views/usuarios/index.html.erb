<p><%= render_breadcrumbs :separator => ' / ' %></p>
<h1>Listado de Usuarios</h1>
<% if notice %>
<p id="notice"><%= notice %></p>
<% end %>


<table>
  <tr>
    <th>Nombre</th>
    <th>Cedula</th>
    <th>Correo</th>
    <th>Unid creditos</th>
    <th>Dependencia</th>
    <th>Rol</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @usuarios.each do |usuario| %>
  <tr>
    <td><%= usuario.nombre %></td>
    <td><%= usuario.cedula %></td>
    <td><%= usuario.correo %></td>
    <td><%= usuario.unid_creditos %></td>
    <td><%= usuario.dependencia.nombre %></td>
    <td><%= usuario.rol %></td>
    <td><%= link_to 'Mostrar', usuario %></td>
    <td><%= link_to 'Editar', edit_usuario_path(usuario) %></td>
    <td><%= link_to 'Eliminar', usuario, :confirm => 'Estas Seguro(a)?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'Nuevo Usuario', new_usuario_path %>
