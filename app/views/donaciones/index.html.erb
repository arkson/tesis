<h1>Listing donaciones</h1>

<table>
  <tr>
    <th>Ejemplar</th>
    <th>Usuario</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @donaciones.each do |donacion| %>
  <tr>
    <td><%= donacion.ejemplar %></td>
    <td><%= donacion.usuario %></td>
    <td><%= link_to 'Show', donacion %></td>
    <td><%= link_to 'Edit', edit_donacion_path(donacion) %></td>
    <td><%= link_to 'Destroy', donacion, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Donacion', new_donacion_path %>
